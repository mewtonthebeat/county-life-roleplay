YCMD:predatbazaru(playerid, params[], help)
{
	new
	    target, cena;
	    
	if(sscanf(params,"dd",target, cena))
	    return SendClientSyntax(playerid, "/predatbazaru [id hr·Ëa] [cena]");
	    
	new vid = GetPlayerVehicleID(playerid);
	if(vid == 0) return SendError(playerid, "NesedÌö v ûiadnom vozidle!");
	
	if(strcmp(vEnum[vid][v_Owner], ReturnName(playerid), false))
        return SendError(playerid, "Toto vozidlo nevlastnÌö!");
        
	if(cena < 1)
	    return SendClientSyntax(playerid, "/predatbazaru [id] [cena]");
	    
    if(!IsPlayerLogged(target))
	    return SendError(playerid, "Tento hr·Ë nie je v hre!");

	if(!IsPlayerNearPlayer(target, playerid, 10.0))
		return SendError(playerid, "Tento hr·Ë je moc Ôaleko!");
	    
	if(GetPlayerFactionType(target) != FACTION_TYPE_BAZAR)
	    return SendError(playerid, "Tento hr·Ë nepracuje v autobazare!");

	new
		id;

	if(B_playerHasMask[playerid] == true) id = I_playerMaskID[playerid];
	else id = playerid;

	SCFM(playerid, COLOR_GREY, "> Odoslal si ûiadosù o predanie vozidla bazaru hr·Ëovi %s za %d$, ûiadosù vypröÌ o 15 sek˙nd!", GetPlayerNameEx(target, USE_MASK),cena);
	SCFM(target, COLOR_GREY, "> %s ti zaslal ûiadosù o predanie vozidla bazaru, ûiadosù vypröÌ o 15 sek˙nd! Cena za auto: %d$", GetPlayerNameEx(playerid, USE_MASK),cena);
	SCFM(target, COLOR_GREY, "> Pre prijatie pouûi prÌkaz /accept playercar %d", id);

	bazarbZiadost[target][playerid] = 1;
	bazarvehid[playerid] = vid;
	bazarcenat[playerid] = cena;
	SetTimerEx("BazarBCancel", 15000, false, "ii", target, playerid);

	return 1;
}

