			SetVehiclesAlarm(vid, 1);

			FormatLog(log_type_other, "%s(%s) vyramoval vozidlo %s(%s).", ReturnName(playerid), ReturnIP(playerid), VehicleNames[GetVehicleModel(vid)-400], vEnum[vid][v_SPZ]);
