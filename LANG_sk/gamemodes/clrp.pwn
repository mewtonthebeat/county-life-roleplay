	mysql_format(MYSQL, query, 1024, "INSERT INTO gm_weed (X, Y, Z, VW, Interior, Owner, UnixPlaced, UnixFinish, DrugId, Hnojivo) VALUES ('%f', '%f', '%f', '%d', '%d', '%e', '%d', '%d', '%d', '%d')",
