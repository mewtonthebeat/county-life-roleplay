	if(times >= (10 + random(12)) || drug_playerConsumedDrug[playerid][drugid] == false)
	{
	    return drug_playerConsumedDrug[playerid][drugid] = false;
	}

	new Float:MaxHP, Float:HP, Float:dontwhen;
