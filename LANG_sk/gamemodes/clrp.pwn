	        return HideSkateboard(playerid);

		static bool:act;
		SetPlayerArmedWeapon(playerid,0);

		if(newkeys & KEY_HANDBRAKE){

			#if MODE_SKATE == 0
				// medium speed
				ApplyAnimation(playerid, "SKATE","skate_run",4.1,1,1,1,1,1,1);
			#else
				// fast speed
				ApplyAnimation(playerid, "SKATE","skate_sprint",4.1,1,1,1,1,1,1);
