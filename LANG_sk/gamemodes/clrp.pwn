				continue;
		    }

		    new
			    tstr[ 8 ],
			    model = 0
			;

	        switch(fdFire[i][fdfire_Steps])
	        {
	            case 6090:
	                model = 18688;
	            case 6070:
	                model = 18691;
	            case 6045:
	                model = 18690;
	            case 6020:
					model = 18689;
	        }

	        format(tstr, sizeof tstr, "%.0f HP", fdFire[i][fdfire_HP]);

			UpdateDynamic3DTextLabelText(fdFire[i][fdfire_LabelID], 0xb03939ff, tstr);

			if(model != 0)
			{
			    DestroyDynamicObject(fdFire[i][fdfire_ObjectID]);
