	vhac_GetVehicleHealth(vehicleid, health);
	GetVehicleDamageStatus(vehicleid, panels, doors, lights, tires);

	////////////////////////////////////////////////////////////////////////////

    mysql_format(
		MYSQL, S_query, sizeof S_query,
