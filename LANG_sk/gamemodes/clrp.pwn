	vEnum[vid][v_bazar_Buyout]   = 0;
	vEnum[vid][v_bazar_DateAdded]   = 0;
	vEnum[vid][v_bazar_BoughtFor]   = 0;
	format(vEnum[vid][v_bazar_Desc], 256, "");
	format(vEnum[vid][v_bazar_AddedBy], MAX_PLAYER_NAME+1, "");

