	if(vEnum[GetPlayerVehicleID(playerid)][v_Nitrous] - 0.555555555555555 < 0.1)
	{
	    vEnum[GetPlayerVehicleID(playerid)][v_Nitrous] = 0.0;
	    RemoveVehicleComponent(GetPlayerVehicleID(playerid), 1010);
	    B_playerNos[playerid] = false;
