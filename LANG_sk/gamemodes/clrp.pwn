	    }
	    else if(playertextid == dealershipTD[playerid][17])
	    {
	        Dealership::NextColor(playerid, 1);
	    }
	    else if(playertextid == dealershipTD[playerid][20])
	    {
	        Dealership::PrevColor(playerid, 2);
	    }
