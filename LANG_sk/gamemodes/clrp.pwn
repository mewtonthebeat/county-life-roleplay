				weedEnum[id][weed_Label] = CreateDynamic3DTextLabel( "{b08a64}Marihuana\n{ffffff}\nrastie...\n/vytrhnut (pre zniËenie)", -1, x, y, z + 0.3, 2.5, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, vw, interior, -1, 5.0, -1, 1);
		}

		case 1:
		{
		    if(unixfinish < gettime())
