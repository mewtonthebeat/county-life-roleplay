				if(I_var >= 50)
				    Achievement::Reward(playerid, ACHIEVEMENT_MOTEL_50DAYS);

				ReloadHouseLabel(i);

				buyBusiness[playerid][0] = 0;
				buyBusiness[playerid][1] = 0;

