func_OnPlayerConnect(playerid)
{

    if(IsPlayerNPC(playerid))
	    return 1;

	new pcount = 0;
	foreach( new x : Player )
	{
		if(IsPlayerNPC(x))
		    continue;

        pcount ++;
	}

	if(pcount > I_gm_playerRecord)
	{
	    I_gm_playerRecord = pcount;

