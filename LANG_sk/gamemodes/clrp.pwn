	if(Anawalt::playerhold[playerid][0] != 0) return 0;

	switch(g_I_playerCheckpoint[playerid])
	{
		case checkp_brigada_krabicePC_1, checkp_brigada_krabicePC_2,
			checkp_brigada_krabiceMG_1, checkp_brigada_krabiceMG_2:

		{
			return 0;
		}
	}

	return 1;
}

