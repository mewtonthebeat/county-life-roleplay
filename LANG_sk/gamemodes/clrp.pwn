{

    new
	    Float:f_finalPrice = (float(fstat_NatankovalLitrov[playerid] + 1) * fstat_CenaZaLiter[playerid]),
	    finalPrice = floatround(f_finalPrice, floatround_floor),
	    id = fstat_BoxID[playerid];

