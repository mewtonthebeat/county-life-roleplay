	}

	FormatLog(log_type_other, "%s (%s - %s) hovori: %s", ReturnName(playerid), ReturnMaster(playerid), ReturnIP(playerid), text);
	printf("[CHAT] %s hovori: %s", ReturnName(playerid), text);
