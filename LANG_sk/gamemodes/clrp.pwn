	if(GetPlayerStaffLevel(playerid) < 2 && !IsPlayerWorking(playerid)) return SendError(playerid,"Nie si v sluzbe!");
	new var; if(sscanf(params,"d",var)) return SendClientSyntax(playerid, "/zbrane [ID]");
