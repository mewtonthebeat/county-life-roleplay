		PlayerSpectatePlayer(playerid, clickedplayerid);
		updateppopis(playerid, "");

		FormatWarnEx(playerid, 2, "AdmCmd:", COLOR_ADMINCMD, "%s zaËal spectatovaù hr·Ëa %s.", GetPlayerNameEx(playerid, USE_PUREIDNAME), GetPlayerNameEx(clickedplayerid, USE_PUREIDNAME));

	}

	return 1;
