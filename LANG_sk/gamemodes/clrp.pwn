	if(B_playerNos[playerid] == false) return 0;
	if(!IsPlayerInAnyVehicle(playerid))
	{
	    RemoveVehicleComponent(GetPlayerVehicleID(playerid), 1010);
	    B_playerNos[playerid] = false;
	    return 0;
