		cellExtra = floatround(cell_Position[i][3],floatround_round);
		break;
	}

	if(!isInRange)
	    return SendError(playerid, "NestojÌö pri cel·ch!");

	if(I_var1 < 1 || I_var1 > 12)
	    return SendClientSyntax(playerid, "/jail [id] [poËet hodÌn] [dÙvod]");

	if(ic_isInJail[I_var] == true)
		return SendError(playerid, "Tento hr·Ë uû je vo v‰zenÌ!");

	ic_isInJail[I_var] = true;
	ic_jailTime[I_var] = gettime() + (I_var1 * 3600);

	SCFM(I_var, 0xD0D0D0FF, "> %s ùa zatkol na %d hodÌn za '%s'! Budeö prepusten˝ %s.", GetPlayerNameEx(playerid, NO_MASK), I_var1, S_var, getdateunix(ic_jailTime[I_var]));

    foreach( new x : Player )
	{
	    if(GetPlayerFaction(x) != GetPlayerFaction(playerid))
	        continue;

		if(!IsPlayerWorking(x) && GetPlayerAdminLevel(playerid) < 4)
