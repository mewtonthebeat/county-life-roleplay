	    return SendError(playerid, "MÙûeö zadaù iba tak˝ poËet hnojiva, ktor˝ m·ö!");

	if(I_var > 3)
	    I_var = 3;

	if(GetPlayerInventoryItem(playerid, weedsinv) < 1)
	    return SendError(playerid, "Nem·ö semienko zvolenej marihuany!");

