	switch(qualityex)
	{
		case 0:
		    return tmpstring;

		case 1:
		{

		    new
				_vardone = 0,
				_varmax  = floatround(strlen(tmpstring) / 2.1, floatround_round)
			;

			while(_vardone < _varmax)
			{

			    new
			        index =
						random(
							strlen(
								tmpstring
							)
						)
				;

				// random character from ascii table
				if(tmpstring[ index ] != ' ')
					tmpstring[ index ] = '?'/*32 + random(222)*/;

				_vardone ++;
			}

		}

		case 2:
		{

		    new
				_vardone = 0,
				_varmax  = floatround(strlen(tmpstring) / 1.2, floatround_round)
			;

			while(_vardone < _varmax)
			{

			    new
			        index =
						random(
							strlen(
								tmpstring
							)
						)
				;

				// random character from ascii table
				if(tmpstring[ index ] != ' ')
					tmpstring[ index ] = '?'/*32 + random(222)*/;

				_vardone ++;
			}

